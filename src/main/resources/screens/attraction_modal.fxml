<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ui.controllers.AttractionModalController"
            prefHeight="600.0" prefWidth="450.0">
    <stylesheets>
        <URL value="@/styling/listview.css"/>
        <URL value="@/styling/styles.css"/>
    </stylesheets>
    <Pane fx:id="filterBarContainer"/>
    <ListView fx:id="cardsContainer" prefHeight="500" prefWidth="450" AnchorPane.topAnchor="50"
              AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0"/>
    <Button xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:id="closeButton" styleClass="yellow-button" text="Close" onAction="#handleReturnValue"
            AnchorPane.topAnchor="10" AnchorPane.rightAnchor="25" alignment="CENTER"/>
</AnchorPane>
