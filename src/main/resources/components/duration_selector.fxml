<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Button?>
<AnchorPane fx:id="rootPane" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefWidth="200" prefHeight="300"
            stylesheets="/styling/styles.css"
fx:controller="ui.controllers.DurationSelectorController">
    <VBox alignment="CENTER" spacing="10">
        <ListView fx:id="timeListView"/>
        <Button text="Select" onAction="#handleSelect"/>
    </VBox>
</AnchorPane>
